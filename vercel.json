{
    "version": 2,
    "builds": [
      { "src": "Backend/index.js", "use": "@vercel/node" },
      { "src": "public/**", "use": "@vercel/static" }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/api/(.*)",
        "dest": "Backend/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "public/$1"
      }
    ],
    "functions": { "api": { "runtime": "vercel-node@0.2" } }
  }
  